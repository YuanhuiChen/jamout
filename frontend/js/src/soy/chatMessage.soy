{namespace jamout.templates.chatMessage}

/**
 * Main Frame
 */
{template .frame}
           <ul>
               <li ng-repeat="message in messages | limitTo: -3 track by $index ">
              
                     [[message.username]]: <span class="chat-messages__message"> [[message.message]] </span>
               </li>
          </ul>

{/template}